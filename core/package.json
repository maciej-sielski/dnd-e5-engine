{
  "name": "@ms-sol/dnd-e5-engine",
  "version": "0.0.1",
  "description": "Basic interfaces and functions to help you build your own RPG games. Based on Dungeons & Dragons 5e.",
  "main": "dist/csj/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "types": "dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/maciej-sielski/dnd-e5-engine.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build-watch": "tsc -p tsconfig.json --watch",
    "build-watch-csj": "tsc -p tsconfig-cjs.json --watch",
    "clean": "rimraf dist && rimraf node_modules",
    "lint": "npx eslint ./src/**/*.ts",
    "lint-fix": "npx eslint --fix ./src/**/*.ts",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.6",
    "eslint": "^9.11.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.6.0"
  },
  "keywords": [
    "games",
    "Dungeons & Dragons",
    "Dungeons & Dragons 5e",
    "dnd",
    "d&d"
  ],
  "author": "Maciej Sielski",
  "license": "MIT",
  "dependencies": {
    "@ms-sol/dice-roll": "^0.1.2",
    "uuid": "^11.0.5"
  }
}
